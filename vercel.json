{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds":[
        {
            "src": "dist/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/app.js"
        }
    ]
}